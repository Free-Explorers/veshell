{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"isBackground": true,
			"label": "embedder run",
			"options": {
				"cwd": "${workspaceFolder}/embedder"
			}
		},
		{
			"type": "dart",
			"command": "dart",
			"args": [
				"run",
				"build_runner",
				"watch"
			],
			"problemMatcher": [
				"$dart-build_runner"
			],
			"group": "build",
			"label": "build_runner watch",
			"detail": "shell",
			"options": {
				"cwd": "${workspaceFolder}/shell"
			},
			"isBackground": true
		}
	]
}